{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
    "Tiff": false
  },
  "rules": {
    "semi": "off",
    "no-undef": "error",
    "arrow-parens": ["error", "always"],
    "block-spacing": "error",
    "eqeqeq": "error",
    "keyword-spacing": "error"
  },
  "parserOptions": {
    "sourceType": "module"
  }

}
